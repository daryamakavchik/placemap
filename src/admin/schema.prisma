// Modify your Keystone config when you want to change this.

datasource mysql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "mysql"
}

generator client {
  provider = "prisma-client-js"
}

model Place {
  id        String  @id @default(uuid())
  name      String  @default("")
  latitude  Float
  longitude Float
  images    Photo[] @relation("Photo_place")
}

model Photo {
  id              String  @id @default(uuid())
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
  place           Place?  @relation("Photo_place", fields: [placeId], references: [id])
  placeId         String? @map("place")

  @@index([placeId])
}
